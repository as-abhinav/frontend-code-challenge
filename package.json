    "build-css": "node-sass-chokidar src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!<rootDir>/src/**/*.test.{js}",
      "!<rootDir>/node_modules/"
